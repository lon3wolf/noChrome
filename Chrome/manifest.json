{    "description": "Blocks Google nagging to get Chrome Browser.",    "name": "!Chrome",    "version": "1.1",    "author": "Sanil Singh Tomar",    "permissions": [        "tabs",        "storage"    ],    "content_scripts": [        {            "matches": [                "*://*.google.com/*",                "*://*.google.com",                "*://*.youtube.com/*",                "*://*.youtube.com",                "*://mail.google.com/*",                "*://mail.google.com/mail",                "*://*.*/*"            ],            "js": [ "borderify.js" ]        }    ],    "icons": {        "20": "icons/border-20-Dark.png",        "40": "icons/border-40-Dark.png"    },    "options_ui": {        "page": "options.html",        "browser_style": true    },    "browser_action": {        "default_icon": {            "20": "icons/border-20-Dark.png",            "40": "icons/border-40-Dark.png"        },        "default_title": "options",        "default_popup": "options.html"    }}